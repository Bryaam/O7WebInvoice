    <script>
        var currentDate = new Date();
        var month = currentDate.getMonth() + 1;
        var day = currentDate.getDate();
        var dateOutput = (day < 10 ? '0' : '') + day + '/' + (month < 10 ? '0' : '') + month + '/' + currentDate.getFullYear();

        $(".currentDateDefault").val(dateOutput);

        $(document).ajaxStop(function () {
            $("#spinner").hide();
        });

        $(document).ajaxSend(function () {
            $("#spinner").show();
        });

        $(document).ajaxStart(function () {
            $("#spinner").show();
        });
      
        $("#spinner").bind("ajaxSend", function () {
            $("#spinner").show();
        }).bind("ajaxComplete", function () {
            $("#spinner").hide();
        }).bind("ajaxStop", function () {
            $("#spinner").hide();
        }).bind("ajaxError", function () {
            $("#spinner").hide();
        });

    </script>
}

@section section_script {
    $("#toHide").hide();
    $("#btnSubmit").click(function() {
        var inputNotFilledYet = false;
        var selectNotFilledYet = false;
        $('input[required]').each(function(){
            if ($(this).val() == ""){
                $(this).focus();
                var curName = "#"+($(this)[0].id)+"Label";
                toastr.error("Campo obligatorio", "Falta completar el campo "+$(curName)[0].textContent, {
                    positionClass: "toast-top-right"
                });
                inputNotFilledYet = true;
                return;
            }
        });
        $('select[required]').each(function(){
            if ($(this)[0].selectedIndex == -1){
                selectNotFilledYet = true;
                return;
            }
        });
        if (inputNotFilledYet || selectNotFilledYet){
            return;
        }else{
            swal({
                title: "Confirmación",
                text: "¿Crear documento?",
                type: "warning",
                showCancelButton: true,
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "Sí",
                closeOnConfirm: false
            }, function() {
                swal({
                    title: "Generado",
                    text: "Documento creado exitosamente",
                    type: "success",
                    confirmButtonText: "Continuar"
                    }, function () {
                         $("#formInvoice").submit();
                });
            });
        }
    });

