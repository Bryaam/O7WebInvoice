    function getCodeByName(source, name) {
        var code = "";
        $.each(source, function(key, value) {
            if (name === value.name) {
                code = value.Value;
                return false;
            }
        });
        return code;
    }

    function getNameByCode(source, code) {
        var name = "";
        $.each(source, function(key, value) {
            if (code === value.Value) {
                name = value.name;
                return false;
            }
        });
        return name;
    }
