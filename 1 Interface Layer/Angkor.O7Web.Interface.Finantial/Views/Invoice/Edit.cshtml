                    url: "/Finantial/Invoice/GetExpirationDate",
                    data: {
                        payment: docType,
                        documentDate: currDate
                    }
                })
                .done(function(result) {
                    var jsonResult = jQuery.parseJSON(result);
                    console.log(jsonResult[0]);
                    $("#fechaVenc").val(jsonResult[0].Content);
                })
                .fail(function(result) {
